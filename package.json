{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "...",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "check-npm-packages": "npm-check --specials=bin,eslint,husky,lint-staged",
    "dev": "vite",
    "format": "yarn lint && yarn lint:styles && yarn sort-imports && yarn prettier",
    "i18n": "yarn i18n:extract-keys && yarn i18n:i18next-resources-for-ts",
    "i18n:extract-keys": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:i18next-resources-for-ts": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/typings/i18n-resources.d.ts",
    "lint": "eslint --fix ./src && tsc --noEmit",
    "lint:styles": "stylelint --fix \"src/**/*.css\"",
    "prepare": "husky",
    "prettier": "prettier --write --list-different \"src/**/*.{js,cjs,mjs,jsx,ts,tsx,json}\"",
    "preview": "vite preview",
    "sort-imports": "import-sort --write \"src/**/*.{js,jsx,ts,tsx,mdx}\" \"!src/**/*.d.ts\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --config ./vite.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:coverage": "nyc --reporter=lcov playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "clsx": "^2.1.1",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "mutative": "^1.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-helmet": "^6.1.0",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.23.1",
    "remeda": "^2.2.0",
    "sha1": "^1.1.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@playwright/test": "^1.44.1",
    "@storybook/addon-a11y": "^8.1.9",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-mdx-gfm": "^8.1.9",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-viewport": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/builder-vite": "^8.1.10",
    "@storybook/manager-api": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/theming": "^8.1.9",
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6",
    "@types/sha1": "^1",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vercel/style-guide": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "browserslist": "^4.23.1",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "i18next-chained-backend": "^4.6.2",
    "i18next-fetch-backend": "^6.0.0",
    "i18next-localstorage-backend": "^4.2.0",
